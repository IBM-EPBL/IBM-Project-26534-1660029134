import random
temperature= random.randint( 60,100)
humidity=random.randint (50,90)
for i in range (30,100):
      if(temperature>60)&(humidity>60):
         print(temperature) 
         print(humidity) 
         print("alarm is on") ;
     else:
         print(temperature) 
         print(humidity) 
         print ("alarm is off");
